{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start -- --inspect",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "debug": "electron-forge start --inspect-electron"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.64",
    "@electron-forge/maker-deb": "^6.0.0-beta.64",
    "@electron-forge/maker-rpm": "^6.0.0-beta.70",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.64",
    "@electron-forge/maker-zip": "^6.0.0-beta.64",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.64",
    "@types/lodash": "^4.14.175",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-window": "^1.8.5",
    "@types/rebass": "^4.0.10",
    "@types/rebass__forms": "^4.0.6",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "css-loader": "^6.4.0",
    "electron": "^19.0.9",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.1",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.4.2",
    "prettier": "^2.4.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@rebass/forms": "^4.0.6",
    "@tanstack/react-query": "^4.0.10",
    "@tanstack/react-virtual": "^3.0.0-beta.17",
    "cors": "^2.8.5",
    "electron-devtools-installer": "^3.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-util": "^0.17.2",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-window": "^1.8.7",
    "rebass": "^4.0.7",
    "styled-components": "^5.3.5",
    "styled-normalize": "^8.0.7"
  },
  "config": {
    "forge": {
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./index.html",
                  "js": "./src/index.tsx",
                  "name": "main_window",
                  "preload": {
                    "js": "./srv/preload.js"
                  }
                }
              ]
            }
          }
        ]
      ],
      "packagerConfig": {
        "protocols": [
          {
            "name": "Electron Fiddle",
            "schemes": [
              "electron-fiddle"
            ]
          }
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron-quick-start"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "mimeType": [
              "x-scheme-handler/electron-fiddle"
            ]
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
